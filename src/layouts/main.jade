extends root

block root
	.main#main
		block main
			!= content
		
		if file.showFeedback
			+b("section").feedback.section.fp-auto-height#feedback(data-anchor='feedback'): +i({ title: "get in touch", text: "Questions or inquiries can easily be sent to us <br>via email through the form submission below" })
	

	if file.fixedPanel
		+b.fixed-panel:+i

block fonts
	link(href="//cloud.webtype.com/css/73ce1cd5-dad0-4708-b74b-591cdd14b2e8.css" rel="stylesheet" type="text/css")


block styles
	link(rel="stylesheet", href=asset("amerigo.min.css"))

block scripts
	script(src=asset("jquery-1.11.0.min.js") defer)
	script(src=asset("amerigo.min.js") defer)

	script(src="https://use.typekit.net/gal7est.js")
	script.
		try{Typekit.load({ async: true });}catch(e){}
